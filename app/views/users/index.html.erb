<div class="album text-muted">
  <div class="container">
    <div class="row">
      <% @users.each do |user| %>
        <div class="col-md-4">
          <div class="card mb-3">
            <div class="card-block">
              <p class="card-text"><%= link_to user.email, user %></p>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>